{
    "expo": {
		"name": "Airtrak",
        "sdkVersion": "35.0.0",
        "version": "2.0.4",
        "orientation": "portrait",
        "privacy": "public",
        "icon": "./assets/images/atk_icon_black.png",
        "facebookScheme": "fb2286396934947742",
        "primaryColor": "#333333",
        "notification": {
            "icon": "./assets/images/atk_icon_black.png",
            "color": "#000000"
        },
        "splash": {
            "image": "./assets/images/fundo_airtracking.png",
            "backgroundColor": "#333333",
            "resizeMode": "cover"
        },
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#333333"
        },
        "packagerOpts": {
            "assetExts": ["otf", "ttf"]
        },
        "ios": {
            "bundleIdentifier": "com.rt.AirTrak"
        },
		"android": {
            "package": "com.rt.AirTrak",
            "versionCode": 13
		},
        "assetBundlePatterns": [
            "**/*.ttf",
            "**/*.otf",
            "**/*.png",
            "**/*.jpg"
        ]
    }
}
