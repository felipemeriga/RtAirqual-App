{
    "expo": {
		"name": "RtAirqual",
        "sdkVersion": "32.0.0",
        "version": "3.0",
        "orientation": "portrait",
        "privacy": "public",
        "icon": "./assets/appIcon.png",
        "facebookScheme": "fb2286396934947742",
        "primaryColor": "#333333",
        "notification": {
            "icon": "./assets/appIcon.png",
            "color": "#000000"
        },
        "splash": {
            "image": "./assets/images/fundo_rt2.png",
            "backgroundColor": "#333333",
            "resizeMode": "cover"
        },
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#333333"
        },
        "packagerOpts": {
            "assetExts": ["otf", "ttf"]
        },
        "ios": {
            "bundleIdentifier": "com.wcandillon.dopurplesky"
        },
		"android": {
            "package": "com.rt.rtairqual",
            "versionCode": 3.0
		},
        "assetBundlePatterns": [
            "**/*.ttf",
            "**/*.otf",
            "**/*.png",
            "**/*.jpg"
        ]
    }
}
